{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bmey.github.io/portfolio/",
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14"
  },
  "lint-staged": {
